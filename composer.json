{
    "name": "xaamin/jwt",
    "description": "PHP Implementation of JSON Web token with RSA",
    "license": "MIT",
    "keywords": [
        "token",
        "jwt",
        "laravel jwt"
    ],
    "authors": [
        {
            "name": "Benjamín Martínez Mateos",
            "email": "xaamin@outlook.com"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "ext-openssl": "*"
    },
    "suggest": {
        "illuminate/support": "To use with Laravel/Lumen"
    },
    "autoload": {
        "psr-4": {
            "Xaamin\\Jwt\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10"
    }
}
